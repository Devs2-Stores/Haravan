<div class="shop-pagination">
	{%- if paginate.pages > 1 %}
	{%- if paginate.previous -%}
	<a href="{{ paginate.previous.url }}" title="Trang trước" aria-label="Trang trước" data-page="{{ paginate.previous.title }}">
		<svg xmlns="http://www.w3.org/2000/svg" version="1.1" xmlns:xlink="http://www.w3.org/1999/xlink" width="16" height="16" x="0" y="0" viewBox="0 0 240.823 240.823" style="enable-background:new 0 0 512 512" xml:space="preserve" class=""><path id="Chevron_Right" d="M57.633,129.007L165.93,237.268c4.752,4.74,12.451,4.74,17.215,0c4.752-4.74,4.752-12.439,0-17.179l-99.707-99.671l99.695-99.671c4.752-4.74,4.752-12.439,0-17.191c-4.752-4.74-12.463-4.74-17.215,0L57.621,111.816C52.942,116.507,52.942,124.327,57.633,129.007z" fill="#000000" data-original="#000000" class=""></path></svg>
	</a>
	{%- endif -%}
	{%- for part in paginate.parts -%}
	{%- if part.is_link -%}
	<a href="{{ part.url }}" title="Trang {{ part.title }}" aria-label="Trang {{ part.title }}" data-page="{{ part.title }}">{{ part.title }}</a>
	{%- else -%}
	<span class="{% if part.title == paginate.current_page %}current{% endif %}">{{part.title}}</span>
	{%- endif -%} 
	{%- endfor -%}
	{%- if paginate.next -%}
	<a href="{{ paginate.next.url }}" title="Trang sau" aria-label="Trang sau" data-page="{{ paginate.next.title }}">
		<svg xmlns="http://www.w3.org/2000/svg" version="1.1" xmlns:xlink="http://www.w3.org/1999/xlink" width="16" height="16" x="0" y="0" viewBox="0 0 240.823 240.823" style="enable-background:new 0 0 512 512" xml:space="preserve" class=""><path id="Chevron_Right_1_" d="M183.189,111.816L74.892,3.555c-4.752-4.74-12.451-4.74-17.215,0c-4.752,4.74-4.752,12.439,0,17.179l99.707,99.671l-99.695,99.671c-4.752,4.74-4.752,12.439,0,17.191c4.752,4.74,12.463,4.74,17.215,0l108.297-108.261C187.881,124.315,187.881,116.495,183.189,111.816z" fill="#000000" data-original="#000000"></path></svg>
	</a>
	{%- endif -%}
	{%- endif -%} 
</div>