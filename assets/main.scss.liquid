			.main-article-share{
				> button svg *, > a svg *{ fill: var(--color2); }
			}
			.main-article-menu.sidebar{
				opacity: 1;
				visibility: visible;
				transform: translate(0, -50%);
			}
		}
	}
	&.open-menu-mobile{
		.headers4{ 
			position: relative;
			z-index: inherit;
			.headers4-bot{
				transform: none;
				visibility: visible;
				opacity: 1;
			}
		}
		.headers3{ 
			position: relative;
			z-index: inherit;
			.headers3-bot{
				transform: none;
				visibility: visible;
				opacity: 1;
			}
		}
		.headers2{ 
			position: relative;
			z-index: inherit;
			.headers2-center{
				transform: none;
				visibility: visible;
				opacity: 1;
			}
		}
		.header {
			position: relative;
			z-index: inherit;
			.header-menu .header-menu-wrap{
				.header-menu-center{
					transform: none;
					visibility: visible;
					opacity: 1;
				}
			}
		}
	}
	&::-webkit-scrollbar {
		width: 0;
	}
	&::-webkit-scrollbar-track {
		background: #ccc;
	}
	&::-webkit-scrollbar-thumb {
		background: var(--color_main);
	}
}

.container-fluid{
	max-width: 1920px;
	margin: 0 auto;
}

.fancybox__content{ 
	padding: 20px !important;
	@include m1200{ padding: 18px !important; }
	@include m1024{ padding: 16px !important; }
	@include m0991{ padding: 14px !important; }
	@include m0767{ padding: 12px !important; }
	@include m0480{ padding: 10px !important; }
	@include m0360{ padding: 5px !important; }
}
.has-spin{
	position: relative;
	@keyframes home_tab-spin {
		from { transform: translate(-50%, -50%) rotate(0deg); }
		to { transform: translate(-50%, -50%) rotate(360deg); }
	}
	&:before{
		content: "";
		background-image: url(data:image/svg+xml;base64,);
		background-size: cover;
		position: absolute;
		top: 50%;
		left: 50%;
		width: 3rem;
		height: 3rem;
		animation: home_tab-spin 5s linear infinite;
	}
}

.slick-slider{
	&:hover{
		.slick-arrow{
			opacity: 1;
			visibility: visible;
		}
	}
	.slick-track{
		padding: 2px 0;
		margin-left: initial;
		div, a, a:focus, div:focus, article:focus, article{
			outline: 0 !important;
			img{ padding: 0; }
		}
	}
	.slick-arrow {
		position: absolute;
		top: 50%;
		background-color: transparent;
		transform: translateY(-50%);
		border: none;
		outline: 0;
		font-size: 20px;
		z-index: 1;
		padding: 0;
		color: #000;
		transition: all .5s;
		background: #DbDbDb;
		box-shadow: 0 0 1px 1px rgba(0, 0, 0, .25);
		padding: 0 !important;
		width: 35px; height: 35px;
		border-radius: 999px; 
		@include dFlex;
		@include m0991 { opacity: 1; visibility: visible; }
		&:hover{
			color: #FFF;
			background: var(--color_main);
			/*&:before{ background: var(--color_main); }
			&:after{ background: var(--color_main); }*/
