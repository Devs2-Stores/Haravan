		.title {
			font-size: 20px;
			font-weight: 700;
			margin-bottom: 20px;
		}
		.close-popup-noti {
			position: absolute;
			right: 10px;
			top: 5px;
			cursor: pointer;
			svg {
				width: 15px;
				height: 15px;
				path {
					fill: #fff;
				}
			}
		}
		ul {
			margin-bottom: 20px;
			li {
				margin-bottom: 10px;
				svg {
					margin-right: 10px;
					path {
						fill: #FFF;
					}
				}
				a {
					color: #fff;
					&:hover {
						color: #fca120;
					}
				}
			}
		}
		.ghichu {
			font-style: italic;
			font-size: 16px;
		}
	}
}
.popup-noti.active {
	.content {
		-ms-transition-delay: 0.1s;
		-webkit-transition-delay: 0.15s;
		transition-delay: 0.1s;
		transform: scale(1);
		-webkit-transform: scale(1);
		-moz-transform: scale(1);
		-ms-transform: scale(1);
		-o-transform: scale(1);
	}
}

