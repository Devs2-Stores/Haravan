$gap-Unit: 1rem;
$white-text: #ECECEC;
$black-text: #333;
$ipad-reponsive: 1000px;
$iphone-reponsive: 600px;
@mixin m1200($string: max) {
	@if $string == min{
		@media(min-width: 1201px){ @content; }
	}@else {
		@media(max-width: 1201px){ @content; }
	}
}
@mixin m1024($string: max) {
	@if $string == min{
		@media(min-width: 1024px){ @content; }
	}@else {
		@media(max-width: 1024px){ @content; }
	}
}
@mixin m0991($string: max) {
	@if $string == min{
		@media(min-width: 991px){ @content; }
	}@else {
		@media(max-width: 991px){ @content; }
	}
}
@mixin m0767($string: max) {
	@if $string == min{
		@media(min-width: 768px){ @content; }
	}@else {
		@media(max-width: 768px){ @content; }
	}
}
@mixin m0480($string: max) {
	@if $string == min{
		@media(min-width: 480px){ @content; }
	}@else {
		@media(max-width: 480px){ @content; }
	}
}
@mixin m0360($string: max) {
	@if $string == min{
		@media(min-width: 360px){ @content; }
	}@else {
		@media(max-width: 360px){ @content; }
	}
}
@mixin sectionPadding {
	padding: 25px 0;
	@include m0767{ padding: 12.5px 0; }
}
@mixin dFlex($flex: flex, $wrap: wrap, $justify: center, $align: center){
	display: $flex;
	flex-wrap: $wrap;
	justify-content: $justify;
	align-items: $align;
}
@mixin wrapText($numberLine){
	overflow: hidden;
	-webkit-line-clamp: $numberLine;
	-webkit-box-orient: vertical;
	display: -webkit-box;
}
@mixin fixMargin {
	@include m0767 { margin-left: -5px; margin-right: -5px; }
	@include m0767(min) { margin-left: -10px; margin-right: -10px; }
}
@mixin fixPadding {
	@include m0767{ padding-left: 5px; padding-right: 5px; }
	@include m0767(min) { padding-left: 10px; padding-right: 10px; }
}
@mixin resetButton{
	background: transparent;
	border: none;
	outline: none;
	padding: 0;
	box-shadow: none; 
}
@mixin hover() {
	@include m0991(min){
		&:hover{
			@content;
		}
	}
}
@mixin buttonStyle1{
	background: #FFF;
	display: inline-block;
	width: fit-content;
	margin: 0 auto;
	text-align: center;
	border: 1px solid #CCC;
	padding: 5px 10px;
	text-transform: uppercase;
	font-weight: bold;
	position: relative;
	transition: color .4s .4s ease-in, border .4s .4s ease-in;
	overflow: hidden;
	@include m0360{ width: 280px; }  
	&:hover {
		color: var(--color_main);
		border-color: var(--color_main);
		border: 1px solid var(--color_main) !important;
		&:after {
			transform: scale(1);
		}
	}
	&:after {
		content: '';
		position: absolute;
		height: 3px;
		width: 100%;
		background: var(--color_main);
		left: 0;
		bottom: 0;
		transition: all 0.4s ease-in;
		transform: scale(0);
		border-radius: var(--border_radius);
	}
}
.buttonStyle1{ @include buttonStyle1; }

@mixin subTitle{
	margin: 0 0 5px;
	display: block;
	color: var(--color_main);
	font-weight: 500;
	font-size: 1rem;
	text-transform: uppercase;
}

@font-face {
	font-family: 'LNTH';
	font-display: swap;
	src: url(//theme.hstatic.net/200000259653/1000680750/14/0250-lnth-righthere.woff2?v=935) format("woff2"), url(//theme.hstatic.net/200000259653/1000680750/14/0250-lnth-righthere.woff?v=935) format("woff"), url(//theme.hstatic.net/200000259653/1000680750/14/0250-lnth-righthere.ttf?v=935) format("ttf"); }

.titleStyle1{
	position: relative;
	color: var(--color_main);
	&.white{
		> h2, > span{ color: #FFF; } 
	}
	> h2{
		color: #000;
		text-align: center;
		position: relative;
		text-transform: uppercase;
		z-index: 1;
		font-weight: bold;
		font-size: 3.5rem;
		@media(max-width: 991px){ font-size: 3rem; }
		@media(max-width: 480px){ font-size: 2.5rem; }
		a{
			&:hover{ color: var(--color_main); }
		}
	}
	> span{
		font-size: 1.5rem;
		color: var(--color_main2);
		display: block;
		text-align: center;
	}
}

.subtitle-fstyle1{
	padding-left: 32px;
	line-height: 1;
	color: var(--color_main);
	position: relative;
	font-size: 1.25rem;
	font-weight: bold;
	margin-bottom: 15px;
	display: block;
	@include m0991{ font-size: 1.1rem; }
	&:before{
		content: "";
		position: absolute;
		left: 0;
		top: 50%;
		transform: translateY(-50%);
		background-image: var(--image_title);
		width: 24px;
		height: 24px;
		background-size: 100% 100%;
	}
}

.home-custom-header{
	display: flex; justify-content: center; align-items: center; flex-direction: column;
	margin-bottom: $gap-Unit * 2;
	h2{
		position: relative; margin-bottom: 15px; padding: 0 85px; color: var(--color_main);
		@media(max-width: $ipad-reponsive){ padding: 0 55px; }
		@media(max-width: $iphone-reponsive){ padding: 0 35px; }
		@media(max-width: 360px){ padding: 0; }
		a{ color: var(--color_main); }
		&:hover{
			a{ color: var(--color_main2); }
		}
	}
	p{ 
		font-size: 1.1rem; 
		max-width: 80%;
		@media(max-width: 600px){ max-width: 100%; }
	}
}

.title-fstyle1{
	color: var(--color_main2);
	font-size: 2.5rem;
	@include m1200{ font-size: 2.0rem; }
	@include m1024{ font-size: 1.5rem; }
}
.button-fstyle1{
	@include resetButton;
	display: inline-block;
	background: #FFF;
	padding: 15px;
	text-transform: uppercase;
	font-weight: bold;
	color: var(--color);
	position: relative;
	transition: all 0.35s 0s ease-out;
	clip-path: polygon(0% 0%, 100% 0, 100% 70%, 90% 100%, 0% 100%);
	font-size: .9rem;
	span, i{ position: relative; z-index: 2; transition: all 0.35s 0s ease-out; }
	i{ margin-left: 5px; }
	@include m1200{ padding: 14px 10px !important; }
	@include m1024{ padding: 13px 10px !important; }
	@include m0991{ padding: 12px 10px !important; }
	@include m0767{ padding: 11px 10px !important; }
	@include m0480{ padding: 10px 10px !important; }
	@include m0360{ padding: 7.5px 10px !important; }
	&:hover{
		color: var(--color2);
		span{ color: var(--color2); }
		i{ transform: rotate(-45deg); color: var(--color2); }
		&:before, &:after{ width: 100%; }
	}
	&:before, &:after{
		content: "";
		position: absolute;
		height: 50%;
		width: 0;
		background: var(--color_main);
		transition: 0.6s cubic-bezier(0.785, 0.135, 0.15, 0.86);
		z-index: 1;
	}
	&:before{ top: 0; left: 0; }
	&:after{ bottom: 0; right: 0; }
}

.zoomContainer{ z-index: 11; }
.w-100{ width: 100%; }
.effect {
	position: relative;
	overflow: hidden;
	width: 100%;
}
.effect-pulse:before,
.effect-pulse:after {
	content: "";
	position: absolute;
	left: 1rem;
	right: 1rem;
	top: 1rem;
	bottom: 1rem;
	-webkit-transition: opacity 0.35s, -webkit-transform 0.35s;
	transition: opacity 0.35s, -webkit-transform 0.35s;
	transition: opacity 0.35s, transform 0.35s;
	transition: opacity 0.35s, transform 0.35s, -webkit-transform 0.35s;
	z-index: 7;
}
.effect-pulse:before {
	border-top: 1px solid white;
	border-bottom: 1px solid white;
	border-radius: var(--border_radius);
	-webkit-transform: scale(0, 1);
	transform: scaleX(0);
}
.effect-pulse:after {
	border-right: 1px solid white;
	border-left: 1px solid white;
	border-radius: var(--border_radius);
	-webkit-transform: scale(1, 0);
	transform: scaleY(0);
}
.effect-pulse:hover:before,
.effect-pulse:hover:after {
	-webkit-transform: scale(1, 1);
	transform: scale(1);
}

@keyframes ping_pong {
	0% { transform: translateY(0) }
	50% { transform: translateY(-20px) }
	to { transform: translateY(0) }
}
@keyframes rotate_wrong {
	0% { transform: rotate(0deg) }
	to { transform: rotate(-1turn) }
}
@keyframes rotate_correct {
	0% { transform: rotate(0deg) }
	to { transform: rotate(1turn) }
}
@keyframes fadeTTB {
	0%	 { transform: translateY(-80px); opacity: 0; }
	25%  { transform: translateY(-60px); opacity: .25;  } 
	50%	 { transform: translateY(-40px); opacity: .5; }
	75%  { transform: translateY(-20px); opacity: .75;  } 
	100% { transform: translateY(0); opacity: 1; }
}
@keyframes fadeBTT {
	0%	 { transform: translateY(40px); opacity: 0; }
	25%  { transform: translateY(30px); opacity: .25;  } 
	50%	 { transform: translateY(20px); opacity: .5; }
	75%  { transform: translateY(10px); opacity: .75;  } 
	100% { transform: translateY(0); opacity: 1; }
}
@keyframes fadeLTR {
	0%	 { transform: translateX(-80px); opacity: 0; }
	25%  { transform: translateX(-60px); opacity: .25;  } 
	50%	 { transform: translateX(-40px); opacity: .5; }
	75%  { transform: translateX(-20px); opacity: .75;  } 
	100% { transform: translateX(0); opacity: 1; }
}
@keyframes fadeRTL {
	0%	 { transform: translateX(80px); opacity: 0; }
	25%  { transform: translateX(60px); opacity: .25;  } 
	50%	 { transform: translateX(40px); opacity: .5; }
	75%  { transform: translateX(20px); opacity: .75;  } 
	100% { transform: translateX(0); opacity: 1; }
}
@keyframes scaleZeroToHero {
	0%	 { transform: scale(0) }
	25%  { transform: scale(.25) }
	50%	 { transform: scale(.5) }
	75%  { transform: scale(.75) }
	100% { transform: scale(1) }
}
@keyframes fade-down {
	0% {
		bottom: 5px;
		opacity: 1
	}
	75% {
		bottom: -15px;
		opacity: 0
	}
}
@media (min-width: 1300px){
	.container, .container-lg, .container-md, .container-sm, .container-xl {
		max-width: 1370px; 
	}
}
@media (min-width: 1500px){
	.container, .container-lg, .container-md, .container-sm, .container-xl {
		max-width: 1440px; 
	}
}
img{ 
	max-width: 100%;
	height: auto;
}
iframe, video{ 
	max-width: 100%;
}
td{
	min-width: 200px;
}
a{
	color: var(--color1);
	&:hover{
		text-decoration: none;
	}
}
ul{
	margin: 0;
	padding: 0;
	list-style: none;
	li{
		list-style: none;
	}
}
input{
	background-clip: padding-box;
	&.form-control{
		&::-webkit-outer-spin-button, &::-webkit-inner-spin-button{
			-webkit-appearance: none;
			-moz-appearance: none;
			appearance: none;
		}
	}
	&[type="radio"]{
		margin-top: 0;
		vertical-align: top;
		width: 18px;
		height: 18px;
		-webkit-appearance: none;
		-moz-appearance: none;
		appearance: none;
		position: relative;
		margin: 0 0.2rem 0 0;
		cursor: pointer;
		outline: none !important;
		border: none !important;
		&:before{
			-webkit-transition: -webkit-transform .4s cubic-bezier(.45,1.8,.5,.75);
			-moz-transition: -moz-transform .4s cubic-bezier(.45,1.8,.5,.75);
			transition: transform .4s cubic-bezier(.45,1.8,.5,.75);
			-webkit-transform: scale(0,0);
			-moz-transform: scale(0,0);
			-ms-transform: scale(0,0);
			-o-transform: scale(0,0);
			transform: scale(0,0);
			content: "";
			position: absolute;
			left: -1px;
			right: 0;
			bottom: 0;
			top: 0;
			margin: auto;
			z-index: 1;
			width: 8px;
			height: 8px;
			background: var(--color1);
			border-radius: 50%;
		}
		&:after{
			content: "";
			position: absolute;
			left: -1px;
			right: 0;
			bottom: 0;
			top: 0;
			margin: auto;
			width: 16px;
			height: 16px;
			background: var(--color2);
			border: 1px solid var(--color1); 
			border-radius: 50%;
		}
		&:checked:before{
			-webkit-transform: scale(1,1);
			-moz-transform: scale(1,1);
			-ms-transform: scale(1,1);
			-o-transform: scale(1,1);
			transform: scale(1,1);
		}
		&:checked:after{
			border: 1px solid var(--color1);
		}
	}
	&[type="checkbox"]{
		margin-top: 0;
		vertical-align: top;
		width: 18px;
		height: 18px;
		-webkit-appearance: none;
		-moz-appearance: none;
		appearance: none;
		position: relative;
		margin: 0 1rem 0 0;
		cursor: pointer;
		outline: none !important;
		border: none !important;
		&:before {
			-webkit-transition: -webkit-transform .4s cubic-bezier(.45,1.8,.5,.75);
			-moz-transition: -moz-transform .4s cubic-bezier(.45,1.8,.5,.75);
			transition: transform .4s cubic-bezier(.45,1.8,.5,.75);
			-webkit-transform: rotate(-45deg) scale(0,0);
			-moz-transform: rotate(-45deg) scale(0,0);
			-ms-transform: rotate(-45deg) scale(0,0);
			-o-transform: rotate(-45deg) scale(0,0);
			transform: rotate(-45deg) scale(0,0);
			content: "";
			position: absolute;
			left: 0;
			right: 0;
			top: .38em;
			margin: auto;
			z-index: 1;
			width: 10px;
			height: 5px;
			border: 2px solid var(--color1);
			border-top-style: none;
			border-right-style: none;
		}
		&:checked:before {
			-webkit-transform: rotate(-45deg) scale(1,1);
			-moz-transform: rotate(-45deg) scale(1,1);
			-ms-transform: rotate(-45deg) scale(1,1);
			-o-transform: rotate(-45deg) scale(1,1);
			transform: rotate(-45deg) scale(1,1);
		}
		&:after {
			content: "";
			position: absolute;
			left: -1px;
			right: 0;
			bottom: 0;
			top: 0;
			margin: auto;
			width: 16px;
			height: 16px;
			background: var(--color2);
			border: 1px solid var(--color1);
			cursor: pointer;
			border-radius: 3px;
		}
		&:checked:after {
			border-color: var(--color1);
		}
	}
	&[type=number] {
		-moz-appearance: textfield;
	}
	&::-webkit-outer-spin-button, &::-webkit-inner-spin-button{
		-webkit-appearance: none;
		margin: 0;
	}
}

@media screen and (-webkit-min-device-pixel-ratio:0) { 
	select,
	textarea,
	input {
		font-size: 16px;
	}
}

.swal-icon--success__line, button.swal-button.swal-button--confirm{
	background: var(--color_main) !important;
}
.swal-icon--success__ring{
	border: 4px solid var(--color_main) !important;
}
.overplay-all {
	@include resetButton;
	opacity: 0;
	visibility: hidden;
	position: fixed;
	background: rgba(45, 45, 50, .8);
	width: 100%;
	height: 100%;
	top: 0 !important;
	left: 0;
	transition: all 0.5s;
	z-index: 98;
}
body, html{
	min-height: 100vh;
	font-size: 15px;
	@include m1200 { font-size: 14.5px; }
	@include m1024 { font-size: 14.0px; }
	@include m0991 { font-size: 13.5px; }
	@include m0767 { font-size: 13.0px; } 
	@include m0480 { font-size: 12.5px; }
	@include m0360 { font-size: 12.0px; }
	&.open-noscroll{
		position: relative;
		overflow: hidden;
		touch-action: none;
		-ms-touch-action: none;
	}
	&.open-overplay{
		.overplay-all{
			opacity: 1;
			visibility: visible;
		}
	}
	&.open-home-sidebar{
		.headers2{ z-index: 98; }
		.home .home-sidebar{
			opacity: 1;
			visibility: visible;
			transform: none;
		}
	}
	&.open-wishlist{
		.shop-wishlist-modal{
			opacity: 1;
			visibility: visible;
			transform: translateY(0);
			z-index: 999;
		}
		.overplay-all{ 
			z-index: 998;
		}
	}
	&.open-compare{
		.shop-compare-modal{
			opacity: 1;
			visibility: visible;
			transform: translateY(0);
			z-index: 999;
		}
		.overplay-all{ 
			z-index: 998;
		}
	}
	&.open-cart{
		.shop-cart-sidebar{
			opacity: 1;
			visibility: visible;
			transform: translateX(0);
			z-index: 999;
		}
		.overplay-all{ 
			z-index: 998;
		}
	}
	&.open-filter-mobile{
		.shop-filter-mobile{
			opacity: 1 !important;
			visibility: visible !important;
			transform: none !important;
		}
	}
	&.open-share{
		.main-article{
			.main-article-share{
				> button svg *, > a svg *{ fill: var(--color2); }
				.main-article-share-popup{
					opacity: 1;
					visibility: visible;
					transform: none;
				}
			}
		}
		.main-product{
			.main-product-wrap{
				.main-product-right{
					.main-product-share{
						.main-product-share-cta{
							background: var(--color_main);
							svg *{ fill: var(--color2); } 
						}
