.home-insta{
	margin: 0 !important;
	position: relative;
	h2{
		@extend .box-title;
	}
	.home-insta-items{
		display: grid; grid-template-columns: repeat(5, 1fr); gap: 1rem; padding: 0 0 1rem;
		> *{ min-width: 0; }
		@media(max-width: $ipad-reponsive){ grid-template-columns: repeat(4, 1fr); }
		@media(max-width: $iphone-reponsive){ grid-template-columns: repeat(2, 1fr); }
		.home-insta-item{
			position: relative;
			a{ 
				display: block;
				overflow: hidden;
				position: relative;
				border-radius: var(--border_radius);
			}
			&:hover{
				&:after, strong, i{ opacity: 1; visibility: visible; }
				strong{ margin-top: -40px; }
				img{ transform: scale(1.25); }
				&:after{ transform: scale(1); }
			}
			&:after{
				content: "";
				position: absolute;
				top: 0;
				left: 0;
				right: 0;
				bottom: 0;
				background: rgba(0, 0, 0, .5);
				border-radius: var(--border_radius);
				transition: ease-in 1000ms;
				z-index: 1;
				opacity: 0;
				visibility: hidden;
				transform: scale(0);
			}
			strong, i{
				position: absolute;
				top: 50%;
				left: 50%;
				transform: translate(-50%, -50%);
				font-size: 2.5em;
				color: #FFF;
				opacity: 0;
				visibility: hidden;
				z-index: 2;
			}
			img{ border-radius: var(--border_radius); transition: ease-in 2400ms; }
			strong{ transition: ease-in 800ms 400ms; font-size: 1.5rem; display: block; width: 100%; padding: 0 1rem; text-align: center; }
			i{ transition: ease-in 800ms 800ms; }
		}
	}
}

footer{ margin: 0 !important; }