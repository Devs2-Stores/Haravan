	.main-product-wrap{
		display: grid;
		grid-template-columns: 40% 1fr;
		gap: 50px;
		position: relative;
		z-index: 99;
		&:has([data-style="style4"]){ 
			grid-template-columns: 55% 1fr; 
			@include m1200{ grid-template-columns: 1fr 1fr; gap: 25px; }
			@include m0991{ grid-template-columns: 1fr; gap: 25px; }
		}
		@include m0991{ grid-template-columns: 1fr; gap: 25px; }
		.main-product-left{
			min-width: 0;
			position: sticky;
			top: 15px;
			height: min-content;
			text-align: center;
			@include m0991{  position: relative; top: 0; height: initial; }
			iframe{ 
				width: 100%;
			}
			a{
				display: block;
				border: none;
				padding: 1px;
				img{ padding: 0; margin: 0 auto; }
			}
			&[data-style="style1"]{
				.slick-prev{ left: 0; }
				.slick-next{ right: 0; }
				.main-product-feature-featured{
					&:not(.slick-slider){
						@include dFlex(flex, nowrap, flex-start, flex-start);
						overflow: hidden;
						a{ 
							min-width: calc(100%/1);
						}
					}
				}
				.main-product-feature-thumbs{
					margin-top: 10px;
					&:not(.slick-slider){
						@include dFlex(flex, nowrap, flex-start, flex-start);
						overflow: hidden;
						a{ 
							min-width: calc(100%/4);
						}
					}
					.slick-list{
						.slick-slide{ 
							display: block;
							border: 1px solid transparent;
							height: 100px;
							margin-right: 5px;
							position: relative;
							&.slick-current.slick-active{ border: 1px solid #F30; }
							@media(max-width: 320px){ height: 60px; }
							img{
								position: absolute;
								top: 50%;
								left: 50%;
								transform: translate(-50%, -50%);
								width: 100%;
								height: 100%;
								object-fit: scale-down;
							}
						}
					}
				}
			}
			&[data-style="style2"]{
				@include dFlex(flex, wrap, flex-start, flex-start);
				.main-product-feature-thumbs{
					width: 20%;
					padding: 30px 0;
					@include m0480{ width: 15%; }
					.slick-arrow{
						&.slick-prev{
							top: 0;
							left: 50%;
							transform: rotate(90deg) translateY(50%);
						}
						&.slick-next{
							bottom: 0;
							left: 50%;
							top: initial;
							right: initial;
							transform: rotate(90deg) translateY(50%);
						}
					}
					.slick-list{
						padding-top: 1px;
						padding-right: 5px;
						.slick-slide{
							display: block;
							border: 1px solid transparent;
							height: 100px;
							margin-bottom: 5px;
							position: relative;
							@include m0991{ height: 90px; }
							@include m0767{ height: 80px; }
							@include m0480{ height: 70px; }
							@include m0360{ height: 60px; }
							&.slick-current.slick-active{ border: 1px solid #F30; }
							img{
								position: absolute;
								top: 50%;
								left: 50%;
								transform: translate(-50%, -50%);
								width: 100%;
								height: 100%;
								object-fit: scale-down;
							}
						}
					}
				}
				.main-product-feature-featured{
					width: 80%;
					@include m0480{ width: 85%; }
					&:not(.slick-slider){
						@include dFlex(flex, nowrap, flex-start, flex-start);
						overflow: hidden;
						a{ 
							min-width: calc(100%/1);
						}
					}
					&.slick-slider{
						.slick-arrow{
							&.slick-prev{
								top: 0 !important;
								transform: translateX(-50%) rotate(90deg);
								left: 50% !important;
							}
							&.slick-next{
								top: inherit !important;
								right: inherit !important;
								bottom: 0 !important;
								transform: translateX(-50%) rotate(90deg);
								left: 50% !important;
							}
						}
						.slick-list{
							.slick-slide{
								opacity: 0;
								&.slick-current.slick-active{ opacity: 1; }
							}
						}
					}
				}
			}
			&[data-style="style3"]{
				.main-product-feature-thumbs{
					a{ 
						display: block;
						margin-bottom: 10px;
					}
				}
			}
			&[data-style="style4"]{
				.main-product-feature-thumbs{
					display: grid;
					grid-template-columns: repeat(2, 1fr);
					gap: 15px;
					&.isOdd{
						a:first-child{ grid-column: 1 / span 2; }
					}
					a{ 
						min-width: 0;
						display: block;
						margin-bottom: 0;
					}
				}
			}
		}
